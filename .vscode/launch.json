{
  "$schema": "https://json.schemastore.org/launch.json",
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "pwa-node",
      "request": "launch",
      "name": "Next.js: Debug server (dev)",
      "runtimeExecutable": "node",
      "runtimeArgs": [
        "--inspect=9229"
      ],
      "program": "${workspaceFolder}/node_modules/next/dist/bin/next",
      "args": [
        "dev"
      ],
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
      "env": {
        "NODE_ENV": "development"
      }
    },
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "Attach to Next.js (9229)",
      "port": 9229,
      "restart": true,
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ]
    }
  ]
}
